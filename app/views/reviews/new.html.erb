<div class="container">
<h1>Create a Review</h1>
<%= simple_form_for [@restaurant, @review] do |f| %>
  <%= f.input :content %>
   <%= f.input :rating %>
  <%= f.submit "Submit review", class: "btn btn-primary" %>
<% end %>
<br>
<%= link_to "Go back", restaurant_path(@restaurant), class: "btn btn-primary"%>
</div>
