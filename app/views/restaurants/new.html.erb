<div class="container">
<h1>Create a Restaurant</h1>
  <%= simple_form_for @restaurant do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
    <%= f.input :phone_number %>
  <%= f.input :category %>
  <%= f.submit %>
<% end %>
<br>
<%= link_to "Go back to List", restaurants_path, class: "btn btn-primary"%>
</div>
